{
	"name": "Shubara",
	"author": "Voltangle",
	"url": "https://github.com/ggoraa/euc.repair/tree/master/shubara",
	"description": "Provides custom functionality and UI for The EUC Wiki.",
	"version": "0.3.0",
	"license-name": "MIT",
	"type": "other",
    "requires": {
        "MediaWiki": ">= 1.43"
    },
    "AutoloadNamespaces": {
        "MediaWiki\\Extension\\Shubara\\": "src/"
    },
    "ExtensionMessagesFiles": {
        "ShubaraMagic": "Shubara.i18n.php"
    },
    "ResourceModules": {
		"ext.shubara.styles": {
            "localBasePath": "resources/ext.shubara",
            "remoteExtPath": "Shubara/resources/ext.shubara",
            "styles": [
				"ext.shubara.css"
			]
		},
		"ext.shubara.js": {
            "localBasePath": "resources/ext.shubara",
            "remoteExtPath": "Shubara/resources/ext.shubara",
            "scripts": [
                "ext.shubara.js"
            ]
		}
    },
    "MessagesDirs": {
        "Shubara": ["i18n"]
    },
    "HookHandlers": {
        "ShubaraHooks": {
            "class": "MediaWiki\\Extension\\Shubara\\Hooks"
        }
    },
    "Hooks": {
        "ParserFirstCallInit": "ShubaraHooks",
        "BeforePageDisplay": "ShubaraHooks"
    },
	"manifest_version": 2
}
